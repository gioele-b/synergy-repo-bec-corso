  "scripts": {
    "start": "npm run ng -- serve app-synergy --port 4200 --open true",
    "build": "ng build app-synergy --configuration=production --source-map=false",
    "gen:page": "npm run ng -- g @synergy/schematics:page",
    "gen:form": "npm run ng -- g @synergy/schematics:form",
    "gen:api": "npm run ng -- g @synergy/schematics:api",
    "gen:apis": "npm run ng -- g @synergy/schematics:apis",
    "gen:deps": "npm run ng -- g @synergy/schematics:upgrade-dependencies",
    "test": "ng test",
    "lint": "eslint -c .eslintrc.js --ext .ts projects/app-synergy",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts projects/app-synergy --fix",
    "analz": "npm run ng -- build app-synergy --configuration=production --stats-json=true && npx webpack-bundle-analyzer ./dist/synergy/stats.json",
    "reinstall": "rmdir /s /q node_modules && rm package-lock.json && npm i",
    "reinstallX": "rm -Rf node_modules && rm -f package-lock.json && npm i",
    "clean": "npm cache clear --force && npm run reinstall",
    "cleanX": "npm cache clear --force && && npm run reinstallX",
    "ng": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "ng1": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng"
  },
